{
    "name": "sebastiandeppisch/heinerenergie-crm",
    "type": "project",
    "description": "Software for the collection of bulk orders for balcony solar panels",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "TODO",
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^2.0.1",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "maxh/php-nominatim": "^2.3",
        "opcodesio/log-viewer": "^3.10",
        "plesk/composer-command-info": "~1.1.0",
        "plesk/ext-laravel-integration": "^7.0",
        "spatie/laravel-data": "^4.15",
        "spatie/laravel-typescript-transformer": "^2.5",
        "tightenco/ziggy": "^2.5",
        "wnx/laravel-sends": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.4",
        "driftingly/rector-laravel": "^2.0.2",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.22.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-watch": "^3.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "plesk/composer-command-info": true
        },
        "platform": {
            "php": "8.4.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}